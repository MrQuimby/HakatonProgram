        <dependency id="Microsoft.Extensions.Configuration.AzureAppConfiguration" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Configuration.AzureKeyVault" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.3" exclude="Build,Analyzers" />
