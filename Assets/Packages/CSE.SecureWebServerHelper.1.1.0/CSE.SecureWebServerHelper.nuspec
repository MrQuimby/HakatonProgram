      <group targetFramework=".NETStandard2.1">
        <dependency id="Microsoft.AspNetCore.Hosting" version="2.2.7" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Server.Kestrel" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Azure.KeyVault" version="3.0.5" exclude="Build,Analyzers" />
